    gem 'jekyll-paginate-v2'
    gem 'jekyll-seo-tag'
    gem 'kramdown'
    gem 'rouge'
    gem 'jekyll-cloudinary'
    gem 'jekyll-sitemap'
    gem 'jekyll-target-blank'
end

